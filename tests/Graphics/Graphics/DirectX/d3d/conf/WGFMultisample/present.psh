Texture2D Tex;
sampler sam;

struct PSIn
{
	float4 pos : SV_Position;
	float2 tex : tex;
};

struct PSOut
{
	float4 Color : SV_Target;
};

cbuffer cb0 : register(b0)
{
	float4 inverseViewport;
	float alpha;
};

PSOut main(PSIn input)
{
	PSOut output;
		
	output.Color = Tex.Sample(sam, input.tex);
	//output.Color = input.pos / 4.0f;
	
	return output;
}
