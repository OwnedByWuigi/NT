        MEND

        MACRO
        TABLE_ENTRY $l,$bias,$numargs
        IMPORT Nt$l
        DCD Nt$l
        MEND

        MACRO
        TABLE_END $n
        EXPORT KiServiceLimit
KiServiceLimit
        DCD     $n + 1
        MEND

        MACRO
        ARGTBL_BEGIN
        EXPORT KiArgumentTable
        ALIGN  4
KiArgumentTable
        MEND

        MACRO
        ARGTBL_ENTRY $arg0, $arg1, $arg2, $arg3, $arg4, $arg5, $arg6, $arg7
        DCB $arg0, $arg1, $arg2, $arg3, $arg4, $arg5, $arg6, $arg7
        MEND

        MACRO
        ARGTBL_END
        END
