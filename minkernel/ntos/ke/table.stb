#include "ksarm.h"

        MACRO
        TABLE_BEGIN1
        MEND

        MACRO
        TABLE_BEGIN2
        AREA |.text|, DATA, READONLY
        MEND

        MACRO
        TABLE_BEGIN3
        MEND

        MACRO
        TABLE_BEGIN4
        EXPORT KiServiceTable
KiServiceTable
        MEND

        MACRO
